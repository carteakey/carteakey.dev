{# <hr />
<h2>Personal</h2>
<p>
  
</p> #}

<div class="mx-auto">
  <div class="mb-8">
    <div class="intro-block">
      <p>Here are some of my personal projects</p>
    </div>
  </div>

  <div class="grid gap-4 md:grid-cols-3">
    {% for project in projects.projects %}
      <article class="card card-hover overflow-hidden flex flex-col group">
        {% if project.image_path %}
          <div class="overflow-hidden">
            {% image project.image_path, project.title, "h-40 w-full object-cover transition-transform duration-300 group-hover:scale-105" %}
          </div>
        {% endif %}

        <div class="flex flex-1 flex-col card-padding gap-2">
          <div class="flex items-start justify-between gap-2">
            <a href="{% if project.url %}{{ project.url }}{% endif %}" title="View Project" class="inline-flex items-center gap-1 text-sm font-bold tracking-tight text-gray-900 link-accent dark:text-gray-100">
              {{ project.title }}
              {% if project.url %}
                {% feather "arrow-up-right", class="w-3.5 h-3.5 opacity-50 group-hover:opacity-100 transition-opacity" %}
              {% endif %}
            </a>
            {% if project.github %}
              <a class="inline-flex items-center text-gray-400 hover:text-gray-700 dark:text-gray-500 dark:hover:text-gray-300 transition-colors shrink-0" title="View Code" href="{{ project.github }}">
                {% feather "github", class="w-4 h-4" %}
              </a>
            {% endif %}
          </div>
          <p class="text-sm leading-relaxed text-gray-600 dark:text-gray-400 line-clamp-2">
            {{ project.description }}
          </p>
          <div class="mt-auto pt-1 flex flex-wrap gap-1.5">
            {% for tag in project.tags %}
              <span class="tag text-[0.6rem]">#{{ tag }}</span>
            {% endfor %}
          </div>
        </div>
      </article>
    {% endfor %}
  </div>
<hr />
<h2>GitHub Activity</h2>
<div class="not-prose">
  <p class="text-gray-600 dark:text-gray-400 mb-4">
    My contribution activity on GitHub over the past year.
  </p>
  
  <div class="card card-padding">
    {% include "components/github-contributions.njk" %}
    <p class="text-xs text-gray-500 dark:text-gray-400 mt-2 text-center">
      Data sourced live from <a href="https://github.com/carteakey" class="hover:underline accent-link">GitHub activity</a>.
    </p>
  </div>
</div>

<hr />
<h2>Projects on GitHub</h2>
<p>
  Here are the rest of my projects on <a href="https://github.com/carteakey?tab=repositories">GitHub</a>.
</p>
<table>
  <thead>
    <tr>
      <th>Updated</th>
      <th>Name</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody id="repoTableBody">
    {% for repo in github.repos %}
    <tr>
      <td class="">
        <time class=" postlist-date">{{ repo.repo_updated | formatDate }}</time>
      </td>

      <td>
        <a href="{{ repo.repo_url }}">
          {{ repo.repo_name }}
        </a>
      </td>
      <td>
        {{ repo.repo_description }}
      </td>


    </tr>
    {% endfor %}
  </tbody>
</table>
</div>
